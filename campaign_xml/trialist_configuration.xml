                        <property>
                            <key>0</key>
                            <label>2 Days</label>
                            <value>2</value>
                        </property>
                        <property>
                            <key>1</key>
                            <label>1 Week</label>
                            <value>7</value>
                        </property>
                        <property>
                            <key>2</key>
                            <label>2 Weeks</label>
                            <value>14</value>
                        </property>
                    </properties>
                </prompt>
                <prompt>

                    <displayLabel>Number of Comparison Cycles</displayLabel>
                    <promptType>single_choice</promptType>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>0</key>
                            <label>2</label>
                        </property>
                        <property>
                            <key>1</key>
                            <label>3</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>4</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <id>random</id>
